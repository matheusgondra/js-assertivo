{
	"env": {
		"es2021": true,
		"node": true,
		"jest": true
	},
	"plugins": [
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"overrides": [
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-unused-vars": "off"
	}
}

